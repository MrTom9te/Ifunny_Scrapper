{
  "manifest_version": 2,
  "name": "Ifunny Scrapper",
  "version": "1.1",
  "description": "Scrapper pro ifunny",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "icons": {
    "48": "icon48.png"
  }
}
